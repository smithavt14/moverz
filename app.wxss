.page__container {
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  width: 100vw;
  min-height: 100vh;
  background-color: #F3F3F3;
  background-size: cover;
  padding-bottom: 200rpx;
}

.btn_area {
  width: 100vw;
  height: 200rpx;
  position: fixed;
  bottom: 0;
  background: white;
  display: flex;
  justify-content: space-around;
  border-top: 1px solid rgba(0,0,0,0.1);
  z-index: 1000 !important;
  align-items: center;
}

.logo_container {
  height: 150rpx;
  width: 100%;
  background: white;
  display: flex;
  justify-content: center;
  align-items: center;
}